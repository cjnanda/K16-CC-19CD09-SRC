<tr>
	<td colspan="2">
	 The following plugins are required to be active: 
	</td>
</tr>
<tr ng-repeat="plugin in requiredPlugins">
	<td>
		<div ng-class="{'incomplete': !plugin.status}">
			<ul>
			<li >
			  <span plugin_id="{{plugin.id}}">{{plugin.name}}</span>
			</li>
		  </ul>
		</div>
	</td>
	<td><div ng-class="{'incomplete': !plugin.status}">{{plugin.status}}</div></td>
</tr>